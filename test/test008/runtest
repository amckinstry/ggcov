#!/usr/bin/env bash

. ../common.sh

init

compile_c foo.c
link foo foo.o

subtest 0
vncdo ./foo 0
run_gcov foo.c
run_tggcov foo.c
compare_lines foo.c

subtest 1
vncdo ./foo 1
run_gcov foo.c
run_tggcov foo.c
compare_lines foo.c

subtest 3
vncdo ./foo 3
run_gcov foo.c
run_tggcov foo.c
compare_lines foo.c

subtest 10
vncdo ./foo 10
run_gcov foo.c
run_tggcov foo.c
compare_lines foo.c
